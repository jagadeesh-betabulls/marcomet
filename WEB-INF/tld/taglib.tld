  <shortname>jobHeaders</shortname>

  <tag>
    <name>JobHeaderTag</name>
    <tagclass>com.marcomet.taglibs.JobHeaderTag</tagclass>
    <info>This tag generates the headers for the workflow forms.</info>
    <attribute>
      <name>jobId</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
  </tag>
  
